<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mymovie">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_logon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_logon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CollectionAdapter.NewCollectionActivity"></activity>
        <activity android:name=".Splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Search.SearchActivity" />
        <activity android:name=".Collection.CollectionActivity" />
        <activity android:name=".ItemInfoActivity.ItemInfoActivity" />
        <activity android:name=".Main.MainActivity"></activity>
    </application>

</manifest>